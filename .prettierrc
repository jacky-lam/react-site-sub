{
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "proseWrap": "always",
    "max-len": [
        "error",
        {
            "code": 80,
            "ignoreComments": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
        }
    ]
}
